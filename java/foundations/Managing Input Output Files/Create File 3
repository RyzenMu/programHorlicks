import java.io.*;

public class createFile3
{
public static void main(String args[])
{

File f3 = new File("javaCreatedFile3.txt");
File f4 = new File("today-10-10-2023.py");
File f5 = new File("hahahahaha.java");

try
{
f3.createNewFile();
f4.createNewFile();
f5.createNewFile();
System.out.println("Three files created");
}
catch(IOException e)
{
System.out.println(e);
System.out.println("Files could not be created");
}
}

}
