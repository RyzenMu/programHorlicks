// other way to classes and objects

class Person:
    def __init__(self, name, age, gender):
        self.name = name
        self.age = age
        self.gender = gender
    
    def talk(self):
        print("Hi , I am ", self.name)
        
    def vote(self):
        if self.age >=18:
            print("I am eligible to vote")
        else:
            print(" I dont vote")
        
p1 = Person("alex", 45, "male")
p2 = Person("mary", 13, "female")

Person.talk(p1)
Person.vote(p1)
Person.talk(p2)
Person.vote(p2)
