#Polymorphism - Operator Overloading

class Point:
    def __init__(self, x, y):
        self.x = x
        self.y = y
        
    def __str__(self):
        return str((self.x, self.y))
        
    def __add__(self, other):
        x = self.x + other.x
        y = self.y + other.y
        return Point(x, y)
        
p1 = Point(3,6)
p2 = Point(4, 6)

print(str(p1))

print(p1 +p2)
